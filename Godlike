    local text = textbox.Text
    if text == "H3LL" then
      screenGui:Destroy()
      loadstring(game:HttpGet("https://raw.githubusercontent.com/GodlikeXH3LL/H3LL-Ninja-Legend-V1/refs/heads/main/By%20Godlike.txt"))()
    else
      textbox.Text = ""
      textbox.PlaceholderText = "Key Incorrect"
